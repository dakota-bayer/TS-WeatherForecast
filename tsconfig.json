{
  "compilerOptions": {
    "target": "ES6", // Specifies ECMAScript target version
    "module": "ES6", // Specifies module code generation
    "moduleResolution": "node",
    "outDir": "./build", // Where to output compiled JavaScript files
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
